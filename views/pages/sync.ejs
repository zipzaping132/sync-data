<script>
    // Call API to subscribe to room syncRoom
    io.socket.post('/subscribe', function (data, jwRes) {
        console.log('Server responded with status code ' + jwRes.statusCode + ' and data: ', data);
    });

    io.socket.on('broadcast', function (data) {
        console.log(data)
        $('#responded').append('<div>' + data.message + '</div>')
    });
</script>

<div id="responded"></div>